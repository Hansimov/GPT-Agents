{
    "configs": {
        "view": {
            "autoPanAndZoomOnLoad": "fit-content"
        },
        "node": {
            "normal": {
                "type": "circle",
                "radius": 20,
                "color": "#88bbff"
            },
            "hover": {
                "color": "#88bbff"
            },
            "label": {
                "visible": true,
                "directionAutoAdjustment": true
            },
            "selectable": true
        },
        "edge": {
            "gap": 12,
            "normal": {
                "color": "gray",
                "animate": true,
                "width": 2,
                "dasharray": "10 6",
                "animationSpeed": 10
            },
            "marker": {
                "source": {
                    "type": "none",
                    "width": 6,
                    "height": 6,
                    "margin": -1,
                    "units": "strokeWidth",
                    "color": null
                },
                "target": {
                    "type": "arrow",
                    "width": 6,
                    "height": 6,
                    "margin": -1,
                    "units": "strokeWidth",
                    "color": null
                }
            },
            "selectable": true
        },
        "path": {
            "visible": true,
            "normal": {
                "width": 12,
                "dasharray": "20 16",
                "animate": true,
                "animationSpeed": 25
            },
            "end": "edgeOfNode",
            "margin": 0,
            "selectable": true
        }
    }
}